{
    "version": 2,
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" },
      { "src": "/(.*)", "dest": "/device-info" },
      { "src": "/(.*)", "dest": "/memory-info" },
      { "src": "/(.*)", "dest": "/flame-info" }
    ]
  }  